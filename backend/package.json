    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.3",
    "fastify": "^4.24.3",
    "fastify-plugin": "^4.5.1",
    "jsonwebtoken": "^9.0.2",
    "yaml": "^2.8.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/json5": "^0.0.29",
    "@types/node": "^20.9.0",
    "@types/prop-types": "^15.7.15",
