    "fastify-plugin": "^4.5.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "prisma": "^5.6.0",
    "tsx": "^4.4.0",
    "typescript": "^5.2.2"
  }
