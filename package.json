{
  "name": "project-library",
  "version": "1.0.0",
  "description": "A comprehensive web-based Project Library platform for AI/ML school projects",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "cd backend && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "type-check": "npm run type-check:backend && npm run type-check:frontend",
    "type-check:backend": "cd backend && npm run type-check",
    "type-check:frontend": "cd frontend && npm run type-check",
    "lint": "npm run lint:frontend",
    "lint:frontend": "cd frontend && npm run lint",
    "db:setup": "cd backend && npm run db:generate && npm run db:migrate",
    "db:reset": "cd backend && npm run db:reset",
    "db:studio": "cd backend && npm run db:studio",
    "clean": "rm -rf node_modules frontend/node_modules backend/node_modules frontend/.next backend/dist"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "@eslint/eslintrc": "^3.1.0",
    "@types/bcrypt": "^6.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/json5": "^0.0.29",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/prop-types": "^15.7.15",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.0.1",
    "eslint": "^9",
    "eslint-config-next": "^16.1.0",
    "postcss": "^8",
    "prisma": "^6.17.1",
    "tailwindcss": "^3.3.0",
    "tsx": "^4.4.0",
    "typescript": "^5"
  },
  "dependencies": {
    "@fastify/cors": "^8.4.0",
    "@fastify/multipart": "^8.0.0",
    "@fastify/static": "^7.0.4",
    "@hookform/resolvers": "^3.3.2",
    "@prisma/client": "^6.17.1",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tanstack/react-query": "^5.8.4",
    "bcrypt": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "fastify": "^4.24.3",
    "fastify-plugin": "^4.5.1",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.544.0",
    "next": "^16.1.0",
    "next-themes": "^0.2.1",
    "ogl": "^1.0.11",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.48.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "yaml": "^2.8.1",
    "zod": "^3.22.4"
  }
}
